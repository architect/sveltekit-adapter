{
  "name": "@architect/sveltekit-adapter",
  "version": "0.0.2",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore  && npm run check-format",
    "test:example-integration": "cd example/hn && npm run test",
    "test": "npm run test:example-integration",
    "postinstall": "cd example/hn && npm install",
    "dev": "rollup -cw",
    "build": "rollup -c",
    "format": "prettier --write . --ignore-path .gitignore ",
    "check-format": "prettier --check . --ignore-path .gitignore ",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "files",
    "index.js",
    "index.cjs",
    "index.cjs.map"
  ],
  "dependencies": {
    "@architect/functions": "^3.14.2"
  },
  "devDependencies": {
    "@architect/eslint-config": "^1.0.1",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@sveltejs/kit": "^1.0.0-next.120",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.2",
    "rollup": "^2.47.0",
    "tape": "^5.3.0"
  },
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "eslintConfig": {
    "extends": [
      "@architect/eslint-config",
      "prettier"
    ],
    "ignorePatterns": [
      "**/*.svelte",
      "/examples"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "prettier": {
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "printWidth": 100,
    "overrides": [
      {
        "files": [
          "*.svelte"
        ],
        "options": {
          "svelteBracketNewLine": true
        }
      }
    ]
  }
}
